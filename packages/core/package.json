{
	"name": "@mapwise/core",
	"version": "1.0.3",
	"private": false,
	"description": "MapWise headless map engine",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js"
		}
	},
	"files": ["dist", "src"],
	"scripts": {
		"typecheck": "tsc",
		"build": "tsc",
		"clean": "rimraf dist .turbo",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"peerDependencies": {
		"maplibre-gl": "^5.0.0",
		"react": "^18.0.0 || ^19.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/geojson": "^7946.0.16",
		"@types/react": "^19.0.0",
		"@vitest/coverage-v8": "^2.1.8",
		"jsdom": "^25.0.1",
		"maplibre-gl": "^5.0.0",
		"rimraf": "^6.0.0",
		"typescript": "^5.0.0",
		"vitest": "^2.1.8"
	},
	"publishConfig": {
		"access": "public"
	}
}
